        "jsCode": "const safeParse = (str) => { try { return JSON.parse(str); } catch {} return null; };\nconst sanitize = (s) => s\n  .replace(/^\\uFEFF/, '')\n  .replace(/\\/\\*[\\s\\S]*?\\*\\//g, '')\n  .replace(/\\/\\/.*$/gm, '')\n  .replace(/\\x0D\\x0A|\\x0D|\\x0A/g, '')\n  .replace(/'([^']+)'(?=\\s*:)/g, '\"$1\"')\n  .replace(/'([^']+)'(?=\\s*,|\\})/g, '\"$1\"')\n  .replace(/,\\s*(?=]})/g, '')\n  .replace(/\\bundefined\\b/g, 'null');\n\nlet whole = items[0].binary.bin;\nlet str   = Buffer.from(whole.data, 'base64').toString('utf8');\nstr = sanitize(str);\n\nlet root = safeParse(str);\nif (!root) return [{ json: { _fixed_file_ok: false, _error: 'Root parse failed' } }];\n\nlet payload = root;\n\nconst rawInside = root.rawJson || root?.[0]?.rawJson || root?.json?.rawJson;\nif (rawInside) {\n  const innerStr = typeof rawInside === 'string' ? sanitize(rawInside) : sanitize(JSON.stringify(rawInside));\n  const innerObj = safeParse(innerStr);\n  if (innerObj) {\n    payload = innerObj;\n  } else {\n    payload = root;\n  }\n}\n\nreturn [{ json: payload, binary: { bin: { data: Buffer.from(JSON.stringify(payload, null, 2), 'utf8').toString('base64') } } }];\n"
